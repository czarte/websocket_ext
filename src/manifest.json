{
  "manifest_version": 3,
  "name": "WebSocket Extension",
  "version": "1.0",
  "permissions": [
    "scripting",
    "tabs"
  ],
  "host_permissions": [
    "http://localhost:*/",
    "https://localhost:*/"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "index.html"
  },
  "web_accessible_resources": [
    {
      "resources": ["*"],
      "matches": ["<all_urls>"]
    }
  ]
}
